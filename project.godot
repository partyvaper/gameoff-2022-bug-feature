[application]

config/name="Bug Feature"
run/main_scene="res://src/Menu.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
boot_splash/image="res://assets/splash.jpg"
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://assets/icon.png"

[autoload]

Dialogic="*res://addons/dialogic/Other/DialogicGameHandler.gd"

[debug]

settings/stdout/verbose_stdout=true
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_parameter=false
gdscript/warnings/integer_division=false

[dialogic]

editor/character_preview_mode=0
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
editor_mode="visual"
editor/last_resources=["res://src/dialogic/RitzTimeline.dtl", "res://src/dialogic/KrakerTimeline.dtl", "res://src/dialogic/Kraker.dch", "res://src/dialogic/Player.dch", "res://src/dialogic/Ritz.dch", ""]
editor/current_timeline_path="res://src/dialogic/KrakerTimeline.dtl"
variables={
"bugs_killed": "0",
"bugs_saved": "0",
"bugs_to_kill": "30",
"bugs_to_save": "15",
"quest1": "unstarted",
"quest2": "unstarted",
"secret": "unset"
}
portrait_mode=0
text/autocolor_names=false
save/autosave_delay=30.0
text/speed=0.01
text/skippable=true
text/autocontinue_delay=1.0
glossary/default_case_sensitive=true
choices/autofocus_first=true
choices/delay=0.2
history/history_system=false
history/full_history=false
history/text_history=false

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/test_width=1280
window/size/test_height=720

[editor]

movie_writer/movie_file="D:/Git/gameoff-2022-bug-feature/movie.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[gui]

theme/use_hidpi=true

